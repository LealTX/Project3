{
  "name": "week20",
  "version": "0.1.0",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.0",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "querystring": "~0.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-materialize": "^2.6.0",
    "react-scripts": "2.1.3",
    "request": "~2.83.0",
    "spotify-web-api-js": "^1.1.1",
    "spotify-web-api-node": "^4.0.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "client": "concurrently \"node server.js\" \"cd client && yarn start\"",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
